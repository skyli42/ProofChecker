import java.util.*;
public class Pt {
	String id;
	ArrayList<Pt>collinear; //points this is collinear with
	public Pt(String id){
		this.id=id.toUpperCase();
		collinear=new ArrayList<Pt>();
	}
	public void addCollinear(Pt other){
		other.collinear.add(this);
		this.collinear.add(other);
		//Yay! It lines up!
	}
}
